{
  "name": "modernize-next-main",
  "version": "1.12.25-test",
  "private": true,
  "pre-commit": {
    "silent": true,
    "run": [
      "check_branch"
    ]
  },
  "scripts": {
    "check_branch": "bash ./pre-commit.sh",
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "test": "next test",
    "version": "echo $npm_package_version",
    "start:dev": "NODE_ENV=development next dev",
    "start:test": "NODE_ENV=test next build",
    "start:prod": "NODE_ENV=production next build",
    "build-docker-prod": "docker build -f Dockerfile --build-arg GITHUB_TOKEN=$READ_GITHUB_TOKEN --build-arg VERSION=$(node -e 'console.log(`${process.env.npm_package_version}`);') -t $(node -e 'console.log(`${process.env.npm_package_name}:${process.env.npm_package_version}`.slice(1));') .",
    "docker-tag": "docker tag $(node -e 'console.log(`${process.env.npm_package_name}:${process.env.npm_package_version}`.slice(1));') 590183752974.dkr.ecr.us-east-2.amazonaws.com/teamsuite-web-portal:$(node -e 'console.log(`${process.env.npm_package_version}`);')",
    "push-docker-image": "npm run build-docker-prod && npm run docker-tag && docker push 590183752974.dkr.ecr.us-east-2.amazonaws.com/teamsuite-web-portal:$(node -e 'console.log(`${process.env.npm_package_version}`);')"
  },
  "dependencies": {
    "@azure/msal-browser": "^3.13.0",
    "@emotion/cache": "^11.11.0",
    "@emotion/react": "^11.11.1",
    "@mui/icons-material": "^5.14.13",
    "@mui/material": "^5.15.15",
    "@mui/material-nextjs": "^5.15.11",
    "@mui/styled-engine-sc": "^6.0.0-alpha.1",
    "@mui/x-date-pickers": "^5.0.15",
    "@mui/x-tree-view": "^6.17.0",
    "@reduxjs/toolkit": "^1.9.7",
    "@tabler/icons-react": "^2.39.0",
    "apexcharts": "3.43.0",
    "axios": "^1.5.1",
    "axios-mock-adapter": "^1.22.0",
    "chance": "^1.1.11",
    "crypto-js": "^4.2.0",
    "date-fns": "^2.30.0",
    "dotenv": "^16.4.5",
    "emoji-picker-react": "^4.5.2",
    "formik": "^2.4.5",
    "i18next": "^23.5.1",
    "jwt-decode": "^4.0.0",
    "lodash": "^4.17.21",
    "moment": "^2.30.1",
    "next": "^14.2.0",
    "next-i18next": "^15.3.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-i18next": "^13.2.2",
    "react-multi-date-picker": "^4.5.2",
    "react-redux": "^8.1.3",
    "react-toastify": "^10.0.5",
    "react-tooltip": "^5.27.0",
    "redux-persist": "^6.0.0",
    "sass": "^1.77.1",
    "simplebar-react": "^3.2.4",
    "stylis-plugin-rtl": "^2.1.1",
    "typescript": "^5.2.2",
    "uuid": "^10.0.0",
    "xlsx": "^0.18.5",
    "yup": "^0.32.11"
  },
  "devDependencies": {
    "@types/node": "20.12.11",
    "eslint": "latest"
  }
}
